{
    "name": "react-es6-starter",
    "version": "0.0.1",
    "description": "A simple starter kit for react with react router, webpack, hot reloading and eslint",
    "main": "app/index.jsx",
    "scripts": {
        "test": "./node_modules/.bin/jest --coverage",
        "start": "webpack-dev-server --content-base app/ --inline --hot --colors"
    },
    "jest": {
        "scriptPreprocessor": "./node_modules/babel-jest",
        "unmockedModulePathPatterns": [
            "<rootDir>/node_modules/react",
            "<rootDir>/node_modules/react-dom",
            "<rootDir>/node_modules/react-addons-test-utils"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "testPathPattern": "**/*.test.js",
        "testFileExtensions": [
            "js", "jsx"
        ],
        "collectCoverage": true,
        "verbose": "true"
    },
    "keywords": [],
    "author": "Johan Nystr√∂m",
    "license": "MIT",
    "dependencies": {
        "bootstrap": "^3.3.6",
        "bootswatch": "^3.3.6",
        "react": "^15.0.1",
        "react-dom": "^15.0.1",
        "react-redux": "^4.4.5",
        "react-router": "^2.3.0",
        "redux": "^3.5.2"
    },
    "devDependencies": {
        "babel-cli": "^6.7.7",
        "babel-core": "^6.7.7",
        "babel-jest": "^12.0.2",
        "babel-loader": "^6.2.4",
        "babel-plugin-react-require": "^2.1.0",
        "babel-polyfill": "^6.8.0",
        "babel-preset-es2015": "^6.6.0",
        "babel-preset-react": "^6.5.0",
        "css-loader": "^0.23.1",
        "eslint": "^2.8.0",
        "eslint-loader": "^1.3.0",
        "eslint-plugin-react": "^5.0.1",
        "jest": "^0.1.40",
        "jest-cli": "^12.0.2",
        "react-addons-test-utils": "^15.0.2",
        "react-hot-loader": "^1.3.0",
        "style-loader": "^0.13.1",
        "webpack": "^1.13.0",
        "webpack-dev-server": "^1.14.1"
    }
}
